{
    "containerDefinitions": [
        {
            "name": "todoapp-django-django",
            "image": "352673354794.dkr.ecr.eu-central-1.amazonaws.com/todoapp-django-django",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "todoapp-django-django-8000-tcp",
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DJANGO_SETTINGS_MODULE",
                    "value": "config.settings_production"
                },
                {
                    "name": "HOST",
                    "value": "todoapp-django.aws.demo.blankzero0.net"
                },
                {
                    "name": "VIRTUAL_HOST",
                    "value": "todoapp-django.aws.demo.blankzero0.net"
                },
                {
                    "name": "LETSENCRYPT_HOST",
                    "value": "todoapp-django.aws.demo.blankzero0.net"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DJANGO_SECRET_KEY",
                    "valueFrom": "arn:aws:ssm:eu-central-1:352673354794:parameter/django-secret-key"
                }
            ]
        }
    ],
    "family": "todoapp-django-django",
    "executionRoleArn": "arn:aws:iam::352673354794:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "memory": "256",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
