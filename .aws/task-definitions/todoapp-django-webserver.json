{
    "containerDefinitions": [
        {
            "name": "todoapp-django-webserver",
            "image": "352673354794.dkr.ecr.eu-central-1.amazonaws.com/todoapp-django-webserver",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "todoapp-django-webserver-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "todoapp-django-webserver-443-tcp",
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [
                {
                    "sourceVolume": "docker-socket",
                    "containerPath": "/tmp/docker.sock",
                    "readOnly": true
                },
                {
                    "sourceVolume": "certs",
                    "containerPath": "/etc/nginx/certs",
                    "readOnly": false
                },
                {
                    "sourceVolume": "vhost",
                    "containerPath": "/etc/nginx/vhost.d",
                    "readOnly": false
                },
                {
                    "sourceVolume": "html",
                    "containerPath": "/usr/share/nginx/html",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "dockerLabels": {
                "com.github.jrcs.letsencrypt_nginx_proxy_companion.nginx_proxy": ""
            }
        },
        {
            "name": "nginx-proxy-acme",
            "image": "nginxproxy/acme-companion",
            "cpu": 0,
            "portMappings": [],
            "essential": false,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [
                {
                    "sourceVolume": "docker-socket",
                    "containerPath": "/var/run/docker.sock",
                    "readOnly": true
                },
                {
                    "sourceVolume": "certs",
                    "containerPath": "/etc/nginx/certs",
                    "readOnly": false
                },
                {
                    "sourceVolume": "vhost",
                    "containerPath": "/etc/nginx/vhost.d",
                    "readOnly": false
                },
                {
                    "sourceVolume": "html",
                    "containerPath": "/usr/share/nginx/html",
                    "readOnly": false
                },
                {
                    "sourceVolume": "acme",
                    "containerPath": "/etc/acme.sh",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DEFAULT_EMAIL",
                    "valueFrom": "arn:aws:ssm:eu-central-1:352673354794:parameter/letsencrypt-mail"
                }
            ],
            "dependsOn": [
                {
                    "containerName": "todoapp-django-webserver",
                    "condition": "START"
                }
            ]
        }
    ],
    "family": "todoapp-django-webserver",
    "executionRoleArn": "arn:aws:iam::352673354794:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [
        {
            "name": "docker-socket",
            "host": {
                "sourcePath": "/var/run/docker.sock"
            }
        },
        {
            "name": "certs",
            "dockerVolumeConfiguration": {
                "scope": "shared",
                "autoprovision": true,
                "driver": "local"
            }
        },
        {
            "name": "vhost",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "driver": "local"
            }
        },
        {
            "name": "html",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "driver": "local"
            }
        },
        {
            "name": "acme",
            "dockerVolumeConfiguration": {
                "scope": "shared",
                "autoprovision": true,
                "driver": "local"
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "memory": "256",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
