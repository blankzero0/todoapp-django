{
    "containerDefinitions": [
        {
            "name": "django",
            "image": "352673354794.dkr.ecr.eu-central-1.amazonaws.com/todoapp-django-django:15da9e22ad83e0ffe91dce6e69b1083a376ed4bf",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "DJANGO_SETTINGS_MODULE",
                    "value": "config.settings_production"
                },
                {
                    "name": "HOST",
                    "value": "todoapp-django.aws.demo.blankzero0.net"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DJANGO_SECRET_KEY",
                    "valueFrom": "arn:aws:ssm:eu-central-1:352673354794:parameter/django-secret-key"
                }
            ],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/todoapp-django",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8000/ || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3
            }
        },
        {
            "name": "webserver",
            "image": "352673354794.dkr.ecr.eu-central-1.amazonaws.com/todoapp-django-webserver:15da9e22ad83e0ffe91dce6e69b1083a376ed4bf",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "webserver-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "webserver-443-tcp",
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "HOST",
                    "value": "todoapp-django.aws.demo.blankzero0.net"
                },
                {
                    "name": "USE_LOCAL_CA",
                    "value": "1"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "CERTBOT_EMAIL",
                    "valueFrom": "arn:aws:ssm:eu-central-1:352673354794:parameter/letsencrypt-mail"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/todoapp-django",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f https://localhost/ || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3
            }
        }
    ],
    "family": "todoapp-django",
    "executionRoleArn": "arn:aws:iam::352673354794:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "memory": "128",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
